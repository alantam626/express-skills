<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
  </head>
  <body>
    <% skills.forEach(function(skill) { %> 
      <li>
        <a href="/skills/<%= skill.id %>"><%= skill.skill %> </a>
        <div>Level: <%=skill.level %></div>
        <div>Time: <%= time %></div>
        <p></p>
      </li>  
    <% }) %>
    <a href="/skills/new">Add Skill</a>
  </body>
</html>
